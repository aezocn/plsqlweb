<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC
        "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "xwork-validator-1.0.2.dtd">

<validators>
	<field name="username">
		<field-validator type="requiredstring">
			<message>Username is required</message>
		</field-validator>
	</field>
	
	<field name="password">
		<field-validator type="requiredstring">
			<message>Password is required</message>
		</field-validator>
	</field>
	
	<field name="databaseip">
		<field-validator type="requiredstring">
			<message>DatabaseIP is required</message>
		</field-validator>
	</field>
	
	<field name="listenport">
		<field-validator type="requiredstring">
			<message>ListenPort is required</message>
		</field-validator>
	</field>
	
	<field name="servername">
		<field-validator type="requiredstring">
			<message>ServerName is required</message>
		</field-validator>
	</field>
	
</validators>
